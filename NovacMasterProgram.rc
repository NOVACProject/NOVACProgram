        BEGIN
            MENUITEM "ppmm",                        ID_UNITOFCOLUMNS_PPMM
            MENUITEM "Molecules/cm2",               ID_UNITOFCOLUMNS_MOLEC_CM2
        END
    END
    POPUP "A&nalysis"
    BEGIN
        MENUITEM "&Flux",                       ID_ANALYSIS_FLUX
        MENUITEM "&Re Evaluate",                ID_ANALYSIS_REEVALUATE
