            MENUITEM "&English",                    ID_SET_LANGUAGE_ENGLISH
            MENUITEM "E&spañol",                    ID_SET_LANGUAGE_ESPA
        END
    END
    POPUP "&View"
    BEGIN
        MENUITEM "&Toolbar",                    ID_VIEW_TOOLBAR
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
        MENUITEM SEPARATOR
        MENUITEM "&Instrument Tab",             ID_MENU_VIEW_INSTRUMENTTAB
